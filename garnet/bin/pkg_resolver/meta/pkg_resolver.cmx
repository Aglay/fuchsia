{
    "program": {
        "binary": "bin/pkg_resolver"
    },
    "sandbox": {
        "features": [
            "config-data",
            "isolated-persistent-storage",
            "root-ssl-certificates"
        ],
        "pkgfs": [
            "install",
            "needs"
        ],
        "services": [
            "fuchsia.amber.Control",
            "fuchsia.logger.LogSink",
            "fuchsia.net.NameLookup",
            "fuchsia.pkg.PackageCache",
            "fuchsia.posix.socket.Provider"
        ]
    }
}
