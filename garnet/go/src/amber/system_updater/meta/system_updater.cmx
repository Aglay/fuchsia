{
    "program": {
        "binary": "bin/system_updater"
    },
    "sandbox": {
        "features": [
            "shell",
            "deprecated-global-persistent-storage"
        ],
        "pkgfs": [
            "packages"
        ],
        "services": [
            "fuchsia.pkg.PackageResolver",
            "fuchsia.cobalt.LoggerFactory",
            "fuchsia.logger.LogSink",
            "fuchsia.process.Launcher"
        ]
    }
}
