import("//build/package.gni")
import("//build/rust/rustc_library.gni")

rustc_library("wlan-bitfields-wrapper") {
  name = "wlan-bitfields-wrapper"
  version = "0.1.0"
  edition = "2018"

  with_unit_tests = true

  deps = [
    "//garnet/lib/rust/wlan-bitfields/",
  ]
}

package("wlan-bitfields-tests") {
  testonly = true

  deps = [
    ":wlan-bitfields-wrapper",
  ]

  tests = [
    {
      name = "wlan_bitfields_wrapper_lib_test"
      environments = basic_envs
    },
  ]
}
