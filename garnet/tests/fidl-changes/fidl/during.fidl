library fidl.test.during;

interface AddMethod {
    ExistingMethod();
    [Transitional]
    NewMethod();
};

interface RemoveMethod {
    ExistingMethod();
    [Transitional]
    OldMethod();
};

interface AddEvent {
    ExistingMethod();
    [Transitional]
    -> NewEvent();
};

interface RemoveEvent {
    ExistingMethod();
    [Transitional]
    -> OldEvent();
};
