import("//build/test/test_package.gni")

test_package("overnet_examples_tests") {
    deps = [
        "echo:bin",
        "interface_passing:bin",
    ]

    meta = [
        {
            path = rebase_path("meta/overnet_echo_run.cmx")
            dest = "overnet_echo_run.cmx"
        },
        {
            path = rebase_path("meta/overnet_interface_passing_run.cmx")
            dest = "overnet_interface_passing_run.cmx"
        },
    ]

    tests = [
        {
            name = "overnet_echo"
        },
        {
            name = "overnet_interface_passing"
        },
    ]
}
