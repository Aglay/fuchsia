import("//build/dart/dart_library.gni")

dart_library("front_end") {
  package_name = "front_end"

  sources = []

  disable_analysis = true

  deps = [
    "//third_party/dart-pkg/pub/convert",
    "//third_party/dart-pkg/pub/crypto",
    "//third_party/dart-pkg/pub/meta",
    "//third_party/dart-pkg/pub/path",
    "//third_party/dart-pkg/pub/source_span",
    "//third_party/dart/pkg/kernel",
  ]

  # fxb/41072: This is to make it a soft-transition and a temporary fix.
  # Will no longer be needed after the linked issue gets addressed.
  if ("True" ==
      exec_script("checkdir.py",
                  [ rebase_path("//third_party/dart/pkg/_fe_analyzer_shared") ],
                  "trim string")) {
    deps += [ "//third_party/dart/pkg/_fe_analyzer_shared" ]
  }
}
