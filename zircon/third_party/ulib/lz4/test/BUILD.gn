import("$zx_build/public/gn/fuzzer.gni")

group("test") {
  testonly = true
  deps = [
    ":lz4-decompress-fuzzer",
    ":lz4-fuzzer",
  ]
}

zx_fuzzer("lz4-decompress-fuzzer") {
  sources = [ "lz4-decompress-fuzzer.cc" ]
  deps = [ "$zx/third_party/ulib/lz4" ]
}

zx_fuzzer("lz4-fuzzer") {
  sources = [ "lz4-fuzzer.cc" ]
  deps = [ "$zx/third_party/ulib/lz4" ]
}
