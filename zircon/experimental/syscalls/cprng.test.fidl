library zz;

[Transport="Syscall"]
protocol Cprng {
    [Internal]
    DrawOnce() -> (status status, vector_void buffer);

    /// Draw from the kernel's CPRNG.
    [Vdsocall]
    Draw(mutable_vector_void buffer) -> ();

    /// Add entropy to the kernel CPRNG.
    AddEntropy(vector_void buffer) -> (status status);
};
