library zz;

[Transport = "Syscall"]
protocol Cprng {
    [Internal]
    cprng_draw_once() -> (status status, vector_void buffer);

    /// Draw from the kernel's CPRNG.
    [Vdsocall]
    cprng_draw() -> (vector_void buffer);

    /// Add entropy to the kernel CPRNG.
    cprng_add_entropy(vector_void buffer) -> (status status);
};
