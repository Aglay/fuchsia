library zz;

[Transport="Syscall"]
protocol Resource {
    /// Create a resource object.
    /// Rights: parent_rsrc must be of type ZX_OBJ_TYPE_RESOURCE and have ZX_RIGHT_WRITE.
    Create(handle<resource> parent_rsrc,
           uint32 options,
           uint64 base,
           usize size,
           string name) ->
        (status status, handle<resource> resource_out);
};
