library zz;

[Transport="Syscall"]
protocol Debug {
    /// Rights: handle must have resource kind ZX_RSRC_KIND_ROOT.
    Read(handle<resource> handle) -> (status status, string buffer, usize actual);

    Write(string buffer) -> (status status);

    /// Rights: resource must have resource kind ZX_RSRC_KIND_ROOT.
    SendCommand(handle<resource> resource, string buffer) -> (status status);
};
