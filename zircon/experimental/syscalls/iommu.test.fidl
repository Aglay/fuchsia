library zz;

[Transport="Syscall"]
protocol Iommu {
    /// Create a new IOMMU object in the kernel.
    /// Rights: resource must have resource kind ZX_RSRC_KIND_ROOT.
    Create(handle<resource> resource, uint32 type, vector_void desc) ->
        (status status, handle<iommu> out);
};
