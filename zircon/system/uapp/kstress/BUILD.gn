import("$zx/kernel/params.gni")

executable("kstress") {
  sources = [
    "main.cpp",
    "stress_test.cpp",
  ]
  deps = [
    "$zx/system/fidl/fuchsia-sysinfo:c",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
  ]
  configs += [ "$zx/public/gn/config:static-libc++" ]
}
