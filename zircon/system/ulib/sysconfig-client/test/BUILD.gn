group("test") {
  testonly = true
  deps = [ ":sysconfig-sync-client-test" ]
}

zx_test("sysconfig-sync-client-test") {
  output_name = "sysconfig-sync-client-test"
  sources = [ "sync-client-test.cc" ]
  include_dirs = [ ".." ]
  deps = [
    "$zx/system/fidl/fuchsia-hardware-nand:c",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/ramdevice-client:static",
    "$zx/system/ulib/sysconfig-client:sysconfig-sync-client",
    "$zx/system/ulib/zxtest",
  ]
}
