{
    include: [ "sdk/lib/diagnostics/syslog/client.shard.cml" ],
    program: {
        binary: "test/storage_operation",
    },
    capabilities: [
        { protocol: "fuchsia.test.Suite" },
    ],
    use: [
        { runner: "gtest_runner" },
        {
            protocol: [ "fuchsia.process.Launcher" ],
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
}
