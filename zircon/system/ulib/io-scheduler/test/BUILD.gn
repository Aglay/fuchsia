group("test") {
  testonly = true
  deps = [ ":io-scheduler-test" ]
}

zx_test("io-scheduler-test") {
  output_name = "iosched"
  sources = [
    "main.cc",
    "stream.cc",
    "unique-op.cc",
  ]
  deps = [
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/fzl",
    "$zx/system/ulib/io-scheduler",
    "$zx/system/ulib/zxtest",
  ]
}
