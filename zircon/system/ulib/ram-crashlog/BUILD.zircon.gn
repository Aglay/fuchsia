import("$zx_build/public/gn/migrated_targets.gni")

zx_library("ram-crashlog") {
  sdk = "source"
  sdk_headers = [ "ram-crashlog/ram-crashlog.h" ]
  sdk_migrated = true
  sources = [ "ram-crashlog.cc" ]
  deps = [ "$zx/third_party/ulib/cksum" ]
  host = true
  kernel = true
  static = true
}

group("tests") {
  testonly = true
  deps = [ "test:tests" ]
}
