import("$zx_build/public/gn/migrated_targets.gni")

zx_library("region-alloc") {
  sdk = "source"
  sdk_headers = [ "region-alloc/region-alloc.h" ]
  sdk_migrated = true
  kernel = true
  static = true
  sources = [
    "region-alloc-c-api.cc",
    "region-alloc.cc",
  ]
  if (!is_kernel) {
    deps = [ "$zx/system/ulib/fbl" ]
  }
}
