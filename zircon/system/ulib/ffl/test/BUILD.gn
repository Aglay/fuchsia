group("test") {
  testonly = true
  deps = [
    ":ffl",
  ]
}

zx_test("ffl") {
  sources = [
    "ffl_tests.cc",
  ]
  deps = [
    "$zx/system/ulib/ffl",
    "$zx/system/ulib/zxtest",
  ]

  if (is_fuchsia) {
    deps += [
      "$zx/system/ulib/fdio",
    ]
  }
}
