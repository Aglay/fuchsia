group("test") {
  testonly = true
  deps = [ ":disk_inspector_unit" ]
}

zx_test("disk_inspector_unit") {
  output_name = "disk-inspector-unit-test"
  sources = [
    "basic_types_test.cc",
    "inspector_transaction_handler_test.cc",
  ]
  deps = [
    "$zx/system/ulib/block-client",
    "$zx/system/ulib/block-client:fake-device",
    "$zx/system/ulib/disk_inspector",
    "$zx/system/ulib/storage/buffer",
    "$zx/system/ulib/storage/operation",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zx",
    "$zx/system/ulib/zxtest",
  ]
}
