import("//build/zircon/migrated_targets.gni")

zx_library("elfload") {
  sdk = "source"
  sdk_headers = [ "elfload/elfload.h" ]
  sdk_migrated = true
  sources = [ "elf-load.c" ]
  deps = [ "$zx/system/ulib/zircon" ]

  # TODO(fxbug.dev/58162): delete the below and fix compiler warnings
  configs += [ "$zx_build_config:Wno-conversion" ]
}

group("tests") {
  testonly = true
  deps = [ "test:tests" ]
}
