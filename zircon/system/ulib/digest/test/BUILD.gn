group("test") {
  testonly = true
  deps = [ ":digest" ]
}

zx_test("digest") {
  sources = [
    "digest.cc",
    "hash-list.cc",
    "merkle-tree.cc",
    "node-digest.cc",
  ]
  deps = [
    "$zx/system/ulib/digest",
    "$zx/system/ulib/fbl",
    "$zx/system/ulib/fdio",
    "$zx/system/ulib/zircon",
    "$zx/system/ulib/zxtest",
    "$zx/third_party/ulib/uboringssl",
  ]
}
