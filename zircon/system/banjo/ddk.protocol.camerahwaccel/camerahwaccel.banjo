library ddk.protocol.camerahwaccel;

[Layout = "ddk-callback"]
protocol HwAccelCallback {
    // This is called by the camera HW Accel drivers (gdc, ge2d, ...), every time a
    // frame is ready to be consumed.
    FrameReady(uint32 buffer_index);
};
