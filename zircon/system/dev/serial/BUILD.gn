group("serial") {
  deps = [
    "aml-uart",
    "ftdi",
    "ftdi:ftdi-fake-usb",
    "serial",
    "serial-async",
    "usb-cdc-acm",
    "usb-cdc-acm:fake-usb-cdc-acm",
  ]
  if (current_cpu == "x64") {
    deps += [ "intel-serialio" ]
    deps += [ "uart16550" ]
  }
}
