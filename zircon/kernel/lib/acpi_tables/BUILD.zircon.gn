zx_library("acpi_tables") {
  kernel = true
  sources = [ "acpi_tables.cc" ]
  public_deps = [
    # <lib/acpi_tables.h> has #include <arch/x86/apic.h>.
    "$zx/kernel/arch/x86:headers",

    # <lib/acpi_tables_test_data.h> has #include <lib/acpi_lite.h>.
    "$zx/kernel/lib/acpi_lite:headers",
  ]

  deps = [
    "$zx/kernel/lib/acpi_lite",
    "$zx/kernel/lib/init",
  ]
}
