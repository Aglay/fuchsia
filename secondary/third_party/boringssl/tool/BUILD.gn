import("../boringssl.gni")

# See //third_party/boringssl/tool/CMakeLists.txt
executable("bssl") {
  visibility = [ "//third_party/boringssl/*" ]
  sources = [
    "args.cc",
    "ciphers.cc",
    "client.cc",
    "const.cc",
    "digest.cc",
    "file.cc",
    "generate_ed25519.cc",
    "genrsa.cc",
    "pkcs12.cc",
    "rand.cc",
    "server.cc",
    "sign.cc",
    "speed.cc",
    "tool.cc",
    "transport_common.cc",
  ]
  configs += [ "//third_party/boringssl:internal_config" ]
  deps = [
    "//third_party/boringssl/crypto",
    "//third_party/boringssl/ssl",
  ]
}
