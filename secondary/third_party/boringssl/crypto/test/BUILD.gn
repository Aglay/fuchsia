import("../../boringssl.gni")

# See //third_party/boringssl/crypto/test/CMakeLists.txt
source_set("test_support") {
  testonly = true
  visibility = [ "//third_party/boringssl/*" ]
  sources = [
    "file_test.cc",
    "malloc.cc",
    "test_util.cc",
  ]
  configs += [ "//third_party/boringssl:test_config" ]
}

source_set("gtest_main") {
  testonly = true
  visibility = [ "//third_party/boringssl/*" ]
  sources = [
    "gtest_main.cc",
  ]
  configs += [ "//third_party/boringssl:test_config" ]
  deps = [
    "//third_party/gtest",
  ]
}
