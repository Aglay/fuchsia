import("../../boringssl.gni")

# See //third_party/boringssl/crypto/err/CMakeLists.txt
source_set("err") {
  visibility = [ "//third_party/boringssl/*" ]
  sources = [
    "$target_gen_dir/err_data.c",
    "err.c",
  ]
  configs += [ "//third_party/boringssl:internal_config" ]
  deps = [
    ":err_data_generate.go",
  ]
}

go_run("err_data_generate.go") {
  sources = [
    "asn1.errordata",
    "bio.errordata",
    "bn.errordata",
    "cipher.errordata",
    "conf.errordata",
    "dh.errordata",
    "digest.errordata",
    "dsa.errordata",
    "ec.errordata",
    "ecdh.errordata",
    "ecdsa.errordata",
    "engine.errordata",
    "evp.errordata",
    "hkdf.errordata",
    "obj.errordata",
    "pem.errordata",
    "pkcs7.errordata",
    "pkcs8.errordata",
    "rsa.errordata",
    "ssl.errordata",
    "x509.errordata",
    "x509v3.errordata",
  ]
  output = "err_data.c"
}
