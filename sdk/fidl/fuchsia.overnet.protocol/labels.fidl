library fuchsia.overnet.protocol;

/// Identifies a single overnet stream between two processes on the Overnet mesh
struct StreamId {
    uint64 id;
};

/// Address of a node on the overlay network.
struct NodeId {
    uint64 id;
};

/// Node-local link label
using LinkId = uint64;
