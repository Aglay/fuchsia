library fuchsia.media;

// Error codes that may be send in epitaphs for audio-related protocols.
enum AudioError : uint32 {
    // The device was disconneted.
    DEVICE_DISCONNECTED = 1;

    // The media session was closed.
    MEDIA_SESSION_CLOSED = 2;
};
