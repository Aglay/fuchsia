{
    children: [
        {
            name: "scenic",
            url: "fuchsia-pkg://fuchsia.com/scenic-stress-tests#meta/scenic.cm",
        },
        {
            name: "root_presenter",
            url: "fuchsia-pkg://fuchsia.com/scenic-stress-tests#meta/root_presenter.cm",
        },
        {
            name: "view",
            url: "fuchsia-pkg://fuchsia.com/scenic-stress-tests#meta/view.cm",
            startup: "eager",
        },
    ],
    offer: [
        // ############### SCENIC ##################
        {
            directory: "/dev/class/display-controller",
            from: "parent",
            to: [ "#scenic" ],
            rights: [ "rw*" ],
        },
        {
            directory: "/dev/class/goldfish-address-space",
            from: "parent",
            to: [ "#scenic" ],
            rights: [ "rw*" ],
        },
        {
            directory: "/dev/class/goldfish-control",
            from: "parent",
            to: [ "#scenic" ],
            rights: [ "rw*" ],
        },
        {
            directory: "/dev/class/goldfish-pipe",
            from: "parent",
            to: [ "#scenic" ],
            rights: [ "rw*" ],
        },
        {
            directory: "/config/vulkan/icd.d",
            from: "parent",
            to: [ "#scenic" ],
            rights: [ "r*" ],
        },
        {
            directory: "/tmp",
            from: "parent",
            to: [ "#scenic" ],
            rights: [ "rw*" ],
        },
        {
            protocol: [
                "/svc/fuchsia.cobalt.LoggerFactory",
                "/svc/fuchsia.hardware.display.Provider",
                "/svc/fuchsia.logger.LogSink",
                "/svc/fuchsia.scheduler.ProfileProvider",
                "/svc/fuchsia.sysmem.Allocator",
                "/svc/fuchsia.tracing.provider.Registry",
                "/svc/fuchsia.ui.input.ImeService",
                "/svc/fuchsia.vulkan.loader.Loader",
            ],
            from: "parent",
            to: [ "#scenic" ],
        },

        // ############## ROOT PRESENTER ##################
        {
            directory: "/config/data",
            from: "parent",
            to: [ "#root_presenter" ],
            rights: [ "r*" ],
        },
        {
            protocol: [
                "/svc/fuchsia.ui.pointerinjector.Registry",
                "/svc/fuchsia.ui.policy.accessibility.PointerEventRegistry",
                "/svc/fuchsia.ui.scenic.Scenic",
            ],
            from: "#scenic",
            to: [ "#root_presenter" ],
        },
        {
            protocol: [
                "/svc/fuchsia.accessibility.ColorTransform",
                "/svc/fuchsia.accessibility.Magnifier",
                "/svc/fuchsia.logger.LogSink",
                "/svc/fuchsia.media.sounds.Player",
                "/svc/fuchsia.recovery.FactoryReset",
                "/svc/fuchsia.sys.Environment",
                "/svc/fuchsia.sys.Loader",
                "/svc/fuchsia.tracing.provider.Registry",
                "/svc/fuchsia.ui.activity.Tracker",
            ],
            from: "parent",
            to: [ "#root_presenter" ],
        },

        // ############### VIEW #####################
        {
            protocol: [ "/svc/fuchsia.logger.LogSink" ],
            from: "parent",
            to: [ "#view" ],
        },
        {
            protocol: [ "/svc/fuchsia.ui.scenic.Scenic" ],
            from: "#scenic",
            to: [ "#view" ],
        },
        {
            protocol: [ "/svc/fuchsia.ui.policy.Presenter" ],
            from: "#root_presenter",
            to: [ "#view" ],
        },
    ],
}
