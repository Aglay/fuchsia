import("//build/package.gni")

executable("blc_bin") {
  output_name = "blc"

  sources = [
    "blc.cc",
  ]

  deps = [
    "//src/camera/calibration/image_writer:image_writer",
  ]
}

package("blc") {
  deps = [
    ":blc_bin",
  ]

  binary = "blc"
}
