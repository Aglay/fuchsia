import("//build/package.gni")

executable("image_writer_bin") {
  output_name = "image_writer"

  sources = [
    "raw12_writer.cc",
  ]

  deps = [
    "//sdk/lib/fidl/cpp",
    "//src/lib/fxl",
    "//zircon/public/lib/fzl",
  ]
}

package("image_writer") {
  deps = [
    ":image_writer_bin",
  ]

  binary = "image_writer"
}
