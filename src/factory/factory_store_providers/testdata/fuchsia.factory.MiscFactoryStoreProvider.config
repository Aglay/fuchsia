{
  "files": [
    {
      "path": "other/misc",
      "dest": "misc.bin",
      "validators": [
        {
          "name": "size",
          "args": 8192
        }
      ]
    },
    {
      "path": "passed_misc_file",
      "dest": "passed/file",
      "validators": [
        {
          "name": "pass"
        }
      ]
    },
    {
      "path": "ignored_misc_file",
      "dest": "ignored"
    }
  ]
}
