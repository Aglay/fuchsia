{
    "program": {
        "binary": "bin/audio_core"
    },
    "sandbox": {
        "dev": [
            "class/audio-input",
            "class/audio-output"
        ],
        "features": [
            "config-data",
            "isolated-persistent-storage",
            "system-temp"
        ],
        "services": [
            "fuchsia.scheduler.ProfileProvider"
        ],
        "system": [
            "data/media"
        ]
    }
}
