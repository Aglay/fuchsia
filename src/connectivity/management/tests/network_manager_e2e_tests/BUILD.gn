import("//build/package.gni")
import("//build/rust/rustc_binary.gni")
import("//build/test/test_package.gni")
import("//build/testing/environments.gni")

group("netemul_empty_binaries") {
  write_file("$root_out_dir/network_manager_e2e_nat_test", "")
}

test_package("network-manager-e2e-tests") {
  deps = [ "nat:network_manager_e2e_nat" ]
  meta = [
    {
      path = rebase_path("nat/meta/bin.cmx")
      dest = "network_manager_e2e_nat.cmx"
    },
  ]
  binaries = [
    {
      name = "network_manager_e2e_nat"
    },
  ]
  tests = [
    {
      name = "network_manager_e2e_nat_test"
      manifest = "nat/meta/test.cmx"
      environments = basic_envs

      # TODO(fxbug.dev/52124): fix this test and re-enable it.
      disabled = true
    },
  ]
}

group("tests") {
  testonly = true
  deps = [
    ":network-manager-e2e-tests",
    "//src/connectivity/network/testing/netemul",
  ]
}
