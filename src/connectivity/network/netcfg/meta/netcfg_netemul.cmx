{
    "program": {
        "args": [
            "--min_severity",
            "DEBUG",
            "--netemul",
            "--config_data",
            "empty.json"
        ],
        "binary": "bin/app"
    },
    "sandbox": {
        "dev": [
            "class/ethernet"
        ],
        "features": [
            "config-data",
            "isolated-persistent-storage"
        ],
        "services": [
            "fuchsia.logger.LogSink",
            "fuchsia.net.dhcp.Server",
            "fuchsia.net.filter.Filter",
            "fuchsia.net.name.LookupAdmin",
            "fuchsia.net.stack.Stack",
            "fuchsia.netstack.Netstack"
        ]
    }
}
