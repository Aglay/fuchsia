{
    "facets": {
        "fuchsia.netemul": {
            "default_url": "fuchsia-pkg://fuchsia.com/mdns-netemul-multi-network-tests#meta/mdns_netemul_multi_network_test_agent.cmx",
            "environment": {
                "children": [
                    {
                        "apps": [
                            {
                                "arguments": [
                                    "--remote"
                                ]
                            }
                        ],
                        "name": "remote",
                        "services": {
                            "fuchsia.net.interfaces.State": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx",
                            "fuchsia.netstack.Netstack": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx",
                            "fuchsia.posix.socket.Provider": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx"
                        },
                        "setup": [
                            {
                                "arguments": [
                                    "-e",
                                    "remote-ep1",
                                    "-i",
                                    "192.168.0.2/24"
                                ],
                                "url": "fuchsia-pkg://fuchsia.com/netemul-sandbox#meta/netstack-cfg.cmx"
                            },
                            {
                                "arguments": [
                                    "-e",
                                    "remote-ep2",
                                    "-i",
                                    "192.168.1.2/24"
                                ],
                                "url": "fuchsia-pkg://fuchsia.com/netemul-sandbox#meta/netstack-cfg.cmx"
                            }
                        ]
                    }
                ],
                "name": "local",
                "services": {
                    "fuchsia.net.interfaces.State": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx",
                    "fuchsia.netstack.Netstack": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx",
                    "fuchsia.posix.socket.Provider": "fuchsia-pkg://fuchsia.com/netstack#meta/netstack.cmx"
                },
                "setup": [
                    {
                        "arguments": [
                            "-e",
                            "local-ep1",
                            "-i",
                            "192.168.0.1/24"
                        ],
                        "url": "fuchsia-pkg://fuchsia.com/netemul-sandbox#meta/netstack-cfg.cmx"
                    },
                    {
                        "arguments": [
                            "-e",
                            "local-ep2",
                            "-i",
                            "192.168.1.1/24"
                        ],
                        "url": "fuchsia-pkg://fuchsia.com/netemul-sandbox#meta/netstack-cfg.cmx"
                    }
                ],
                "test": [
                    {
                        "arguments": [
                            "--local"
                        ]
                    }
                ]
            },
            "networks": [
                {
                    "endpoints": [
                        {
                            "name": "local-ep1"
                        },
                        {
                            "name": "remote-ep1"
                        }
                    ],
                    "name": "net1"
                },
                {
                    "endpoints": [
                        {
                            "name": "local-ep2"
                        },
                        {
                            "name": "remote-ep2"
                        }
                    ],
                    "name": "net2"
                }
            ]
        }
    },
    "program": {
        "binary": "bin/app"
    },
    "runner": "fuchsia-pkg://fuchsia.com/netemul_runner#meta/netemul_runner.cmx"
}
