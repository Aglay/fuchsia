{
    program: {
        binary: "test/sampler_bin_test",
    },
    capabilities: [
        { protocol: "fuchsia.test.Suite" },
    ],
    use: [
        { runner: "rust_test_runner" },
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
                "fuchsia.sys.Launcher",
            ],
        },
        {
            storage: "temp",
            path: "/tmp",
        },
        {
            protocol: "fuchsia.sys2.Realm",
            from: "framework",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
}
