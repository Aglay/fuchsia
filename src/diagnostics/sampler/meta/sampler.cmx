{
    "program": {
        "args": [
            "lapis",
            "--minimum-sample-rate-sec",
            "1"
        ],
        "binary": "bin/launcher"
    },
    "sandbox": {
        "features": [
            "config-data"
        ],
        "services": [
            "fuchsia.cobalt.LoggerFactory",
            "fuchsia.diagnostics.ArchiveAccessor",
            "fuchsia.logger.Log",
            "fuchsia.logger.LogSink"
        ]
    }
}
