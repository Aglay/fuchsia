{
    "program": {
        "args": [
            "--config-path",
            "/config/data/archivist_config.json"
        ],
        "binary": "bin/archivist"
    },
    "sandbox": {
        "features": [
            "config-data",
            "global-data",
            "isolated-persistent-storage"
        ],
        "services": [
            "fuchsia.sys.internal.ComponentEventProvider",
            "fuchsia.sys.internal.LogConnector"
        ]
    }
}
