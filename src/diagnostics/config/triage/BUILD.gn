import("//src/diagnostics/triage/build/triage_config_test.gni")

group("triage") {
  testonly = true
  deps = [ ":tests" ]
}

group("tests") {
  testonly = true
  deps = [ ":triage_configs_test($host_toolchain)" ]
}

if (is_host) {
  triage_config_test("triage_configs_test") {
    sources = [
      "crashes.triage",
      "disk.triage",
      "intl.triage",
      "stash.triage",
      "stash_secure.triage",
      "timezone.triage",
    ]
    source_dir = "."
  }
}
