import("//build/fuzzing/fuzzer.gni")

group("fuzzer") {
  testonly = true
  deps = [
    ":inspect_fuzzers",
  ]
}

fuzzers_package("inspect_fuzzers") {
  fuzzers = [ ":inspect_reader_fuzzer" ]
}

fuzzer("inspect_reader_fuzzer") {
  sources = [
    "inspect_reader.cc",
  ]
  deps = [
    "//zircon/public/lib/inspect",
  ]
}
