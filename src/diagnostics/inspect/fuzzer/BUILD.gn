import("//build/fuzzing/fuzzer.gni")

group("fuzzer") {
  testonly = true
  deps = [ ":inspect-fuzzers" ]
}

fuzzers_package("inspect-fuzzers") {
  fuzzers = [ ":inspect-reader-fuzzer" ]
}

fuzzer("inspect-reader-fuzzer") {
  sources = [ "inspect_reader.cc" ]
  deps = [ "//zircon/public/lib/inspect" ]
}
