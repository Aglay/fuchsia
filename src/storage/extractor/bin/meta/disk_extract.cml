{
    program: {
        binary: "bin/disk-extract",
    },
    capabilities: [
        { protocol: "fuchsia.fs.Admin" },
    ],
    use: [
        { runner: "elf" },
        { protocol: "fuchsia.logger.LogSink" },
        {
            directory: "dev-class-block",
            rights: [ "rw*" ],
            path: "/dev/class/block",
        },
        {
            directory: "deprecated-tmp",
            rights: [
                "admin",
                "rw*",
            ],
            path: "/tmp",
        },
    ],
}
