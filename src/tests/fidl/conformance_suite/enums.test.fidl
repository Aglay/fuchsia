library conformance;

strict enum StrictEnum {
    FIRST = 1;
    SECOND = 2;
};

enum FlexibleEnum {
    FIRST = 1;
    SECOND = 2;
};

enum FlexibleEnumUint64 : uint64 {
    FIRST = 1;
    SECOND = 2;
};

struct StrictEnumStruct {
    StrictEnum e;
};

struct FlexibleEnumStruct {
    FlexibleEnum e;
};

struct FlexibleEnumUint64Struct {
    FlexibleEnumUint64 e;
};
