library conformance;

strict bits StrictBits : uint32 {
    A = 0x1;
    B = 0x2;
};

bits FlexibleBits : uint32 {
    A = 0x1;
    B = 0x2;
};

bits FlexibleBitsUint64 : uint64 {
    A = 0x1;
    B = 0x2;
};

struct StrictBitsStruct {
    StrictBits b;
};

struct FlexibleBitsStruct {
    FlexibleBits b;
};

struct FlexibleBitsUint64Struct {
    FlexibleBitsUint64 b;
};
