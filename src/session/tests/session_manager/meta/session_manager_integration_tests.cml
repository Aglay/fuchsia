{
    include: [ "sdk/lib/diagnostics/syslog/client.shard.cml" ],
    program: {
        binary: "test/session_manager_integration_tests",
    },
    collections: [
        {
            name: "session",
            durability: "transient",
        },
    ],
    capabilities: [
        { protocol: "fuchsia.test.Suite" },
    ],
    use: [
        { runner: "rust_test_runner" },
        {
            protocol: "fuchsia.sys2.Realm",
            from: "framework",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
}
