{
    program: {
        binary: "bin/input_session",
    },
    use: [
        { runner: "elf" },
        {
            directory: "dev-class-input-report",
            from: "parent",
            rights: [ "r*" ],
            path: "/dev/class/input-report",
        },
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
        },
    ],
}
