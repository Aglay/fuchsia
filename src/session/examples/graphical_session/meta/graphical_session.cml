{
    program: {
        binary: "bin/graphical_session",
    },
    use: [
        { runner: "elf" },
        {
            // The Scenic service allows the session to display images.
            protocol: "/svc/fuchsia.ui.scenic.Scenic",
        },
        {
            protocol: "/svc/fuchsia.logger.LogSink",
        },
    ],
    expose: [
        {
            protocol: "/svc/fuchsia.ui.app.ViewProvider",
            from: "self",
        }
    ]
}
