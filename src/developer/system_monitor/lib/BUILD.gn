import("//build/test.gni")

source_set("lib") {
  sources = [ "gt_log.cc" ]
}

group("tests") {
  testonly = true
  deps = [
    ":gt_log_tests",
    "//src/developer/system_monitor/lib/dockyard:tests",
  ]
}

test("gt_log_tests") {
  testonly = true

  sources = [ "gt_log_test.cc" ]

  deps = [
    ":lib",
    "//sdk/lib/syslog/cpp:backend_legacy",
    "//src/lib/fxl/test:gtest_main",
    "//third_party/googletest:gtest",
  ]
}
