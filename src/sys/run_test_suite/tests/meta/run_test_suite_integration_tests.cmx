{
    "facets": {
        "fuchsia.test": {
            "injected-services": {
                "fuchsia.test.manager.Harness": "fuchsia-pkg://fuchsia.com/component_manager_for_test#meta/component_manager_for_test.cmx"
            }
        }
    },
    "include": [
        "sdk/lib/diagnostics/syslog/client.shard.cmx"
    ],
    "program": {
