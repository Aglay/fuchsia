{
    program: {
        binary: "bin/test_driver",
    },
    collections: [
        {
            name: "coll",
            durability: "transient",
        },
    ],
    capabilities: [
        { protocol: "fuchsia.test.Suite" },
    ],
    use: [
        { runner: "rust_test_runner" },
        {
            protocol: [ "fuchsia.logger.LogSink" ],
        },
        {
            protocol: "fuchsia.sys2.Realm",
            from: "framework",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: [ "#coll" ],
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "self",
        },
    ],
}
