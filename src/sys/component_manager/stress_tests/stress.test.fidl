library test.componentmanager.stresstests;

/// The way for parent to conrtol lifecycle of its child in a stress test.
[Discoverable]
protocol Lifecycle {
    /// Component will gracefully exit.
    Stop();

    /// Fired when the server is connected
    -> OnConnected();
};
