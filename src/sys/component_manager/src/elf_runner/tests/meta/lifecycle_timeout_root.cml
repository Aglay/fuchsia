{
    children: [
        {
            name: "custom-timeout-child",
            url: "fuchsia-pkg://fuchsia.com/elf_runner_lifecycle_test#meta/lifecycle_timeout_node.cm",
            startup: "eager",
            environment: "#custom-timeout",
        },
        {
            name: "inherited-timeout-child",
            url: "fuchsia-pkg://fuchsia.com/elf_runner_lifecycle_test#meta/lifecycle_timeout_node.cm",
            startup: "eager",
        },
    ],
    use: [
        {
            protocol: [ "/svc/fuchsia.process.Launcher" ],
        },
        {
            protocol: [ "/svc/fuchsia.sys2.Realm" ],
            from: "framework",
        },
    ],
    offer: [
        {
            protocol: [ "/svc/fuchsia.logger.LogSink" ],
            from: "parent",
            to: [
                "#custom-timeout-child",
                "#inherited-timeout-child",
            ],
        },
    ],
    environments: [
        {
            name: "custom-timeout",
            extends: "realm",
            __stop_timeout_ms: 1000,
        },
    ],
}
