{
    "program": {
        "binary": "bin/memfs",
    },
    "use": [
        { "runner": "elf" },
        { "protocol": "/svc/fuchsia.logger.LogSink" },
    ],
    "expose": [
        {
            "directory": "/svc/fuchsia.io.Directory",
            "from": "self",
            "as": "/memfs",
            "rights": ["rw*"],
        }
    ],
}
