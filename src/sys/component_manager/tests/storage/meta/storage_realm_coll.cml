{
    program: {
        binary: "bin/storage_realm_coll",
    },
    children: [
        {
            name: "memfs",
            url: "fuchsia-pkg://fuchsia.com/storage_integration_test#meta/memfs.cm",
            startup: "eager",
        },
    ],
    collections: [
        {
            name: "coll",
            durability: "transient",
        },
    ],
    capabilities: [
        {
            storage: "mystorage",
            from: "#memfs",
            backing_dir: "memfs",
        },
    ],
    use: [
        { runner: "elf" },
        {
            protocol: "fuchsia.sys2.Realm",
            from: "framework",
        },
        {
            protocol: "fidl.test.components.Trigger",
            from: "framework",
        },
        {
            protocol: "/svc/fuchsia.logger.LogSink",
            from: "parent",
        },
    ],
    offer: [
        {
            storage: "data",
            from: "#mystorage",
            to: [ "#coll" ],
        },
        {
            protocol: "/svc/fuchsia.logger.LogSink",
            from: "parent",
            to: [
                "#coll",
                "#memfs",
            ],
        },
    ],
}
