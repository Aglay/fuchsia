{
    program: {
        binary: "bin/storage_user",
    },
    capabilities: [
        {
            directory: "data",
            rights: [ "rw*" ],
            path: "/data",
        },
    ],
    use: [
        { runner: "elf" },
        { storage: "data" },
        { protocol: "fuchsia.logger.LogSink" },
    ],
    expose: [
        {
            directory: "data",
            from: "self",
        },
    ],
}
