{
    program: {
        binary: "bin/use_dir_rights",
    },
    use: [
        { runner: "elf" },

        // This is invalid and should fail.
        {
            directory: "/read_only",
            rights: [ "rw*" ],
        },
        {
            directory: "/read_write",
            rights: [ "rw*" ],
        },
        {
            directory: "/read_write_dup",
            rights: [ "rw*" ],
        },
        {
            directory: "/read_exec",
            rights: [ "rx*" ],
        },
        {
            directory: "/read_admin",
            rights: [
                "admin",
                "r*",
            ],
        },
        {
            directory: "/read_only_after_scoped",
            rights: [ "r*" ],
        },
    ],
    expose: [
        {
            protocol: "/svc/fidl.test.components.Trigger",
            from: "self",
        },
    ],
}
