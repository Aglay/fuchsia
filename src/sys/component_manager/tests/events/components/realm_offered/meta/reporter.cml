{
    program: {
        binary: "bin/realm_offered_reporter",
    },
    use: [
        {
            protocol: "/svc/fidl.examples.routing.echo.Echo",
            from: "framework",
        },

        // Offered by the parent realm "nested"
        {
            protocol: [
                "/svc/fidl.test.components.Trigger",
                "/svc/fuchsia.sys2.BlockingEventSource",
            ],
        },
        {
            event: "started_nested",
            from: "realm",
        },
        {
            event: "resolved",
            from: "framework",
        },
        { runner: "elf" },
    ],
}
