{
    program: {
        binary: "bin/nested_reporter",
    },
    children: [
        {
            name: "child_a",
            url: "fuchsia-pkg://fuchsia.com/events_integration_test#meta/stub_component.cm",
            startup: "eager",
        },
        {
            name: "child_b",
            url: "fuchsia-pkg://fuchsia.com/events_integration_test#meta/stub_component.cm",
            startup: "eager",
        },
        {
            name: "child_c",
            url: "fuchsia-pkg://fuchsia.com/events_integration_test#meta/stub_component.cm",
            startup: "eager",
        },
    ],
    use: [
        { runner: "elf" },
        { protocol: "/svc/fuchsia.sys2.BlockingEventSource" },
        {
            protocol: [ "/svc/fidl.examples.routing.echo.Echo" ],
            from: "framework",
        },
        {
            event: [
                "resolved",
                "started",
            ],
            from: "framework",
        },
    ],
    offer: [
        // Offer the ELF runner to children.
        {
            runner: "elf",
            from: "realm",
            to: [
                "#child_a",
                "#child_b",
                "#child_c",
            ],
        },
    ],
}
