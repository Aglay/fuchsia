{
    program: {
        binary: "bin/async_reporter",
    },
    collections: [
        {
            name: "coll",
            durability: "transient",
        },
    ],
    use: [
        { runner: "elf" },
        { protocol: "/svc/fuchsia.sys2.EventSource" },
        {
            protocol: [
                "/svc/fidl.examples.routing.echo.Echo",
                "/svc/fuchsia.sys2.Realm",
            ],
            from: "framework",
        },
        {
            event: [
                "destroyed",
                "started",
            ],
            from: "framework",
        },
    ],
}
