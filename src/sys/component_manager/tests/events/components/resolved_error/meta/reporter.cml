{
    program: {
        binary: "bin/resolved_error_reporter",
    },
    children: [
        {
            name: "child_a",
            url: "fuchsia-pkg://fuchsia.com/events_integration_test#meta/does_not_exist.cm",
            startup: "lazy",
        },
    ],
    use: [
        { runner: "elf" },
        { protocol: "fuchsia.sys2.EventSource" },
        {
            protocol: [
                "/svc/fuchsia.logger.LogSink",
                "fidl.examples.routing.echo.Echo",
                "fuchsia.sys2.Realm",
            ],
            from: "framework",
        },
        {
            event: [
                "resolved",
                "started",
            ],
            from: "framework",
        },
    ],
}
