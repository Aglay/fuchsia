{
    program: {
        binary: "bin/dynamic_child_reporter",
    },
    collections: [
        {
            name: "coll",
            durability: "transient",
        },
    ],
    use: [
        { runner: "elf" },
        {
            protocol: [
                "/svc/fuchsia.sys2.BlockingEventSource",
                "/svc/fuchsia.sys2.Realm",
                "/svc/fuchsia.test.hub.HubReport",
            ],
            from: "framework",
        },
        {
            directory: "/hub",
            from: "framework",
            rights: [ "r*" ],
        },
        {
            from: "framework",
            event: "stopped",
        },
        {
            from: "framework",
            event: "marked_for_destruction",
        },
        {
            from: "framework",
            event: "destroyed",
        },
    ],
    offer: [
        // Offer the ELF runner to children.
        {
            runner: "elf",
            from: "realm",
            to: [ "#coll" ],
        },
    ],
}
