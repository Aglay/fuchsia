{
    program: {
        binary: "bin/policy_service",
    },
    capabilities: [
        {
            protocol: [
                "test.policy.Restricted",
                "test.policy.Unrestricted",
            ],
        },
        {
            directory: "test",
            rights: [ "r*" ],
            path: "/test",
        },
    ],
    use: [
        { runner: "elf" },
    ],
    expose: [
        {
            protocol: [
                "test.policy.Restricted",
                "test.policy.Unrestricted",
            ],
            from: "self",
        },
        {
            directory: "test",
            from: "self",
        },
    ],
}
