{
    program: {
        binary: "bin/cap_getter",
    },
    capabilities: [
        { protocol: "test.policy.Access" },
    ],
    use: [
        { runner: "elf" },
        {
            protocol: [
                "test.policy.Restricted",
                "test.policy.Unrestricted",
            ],
        },
        {
            directory: "restricted",
            rights: [ "rw*" ],
            path: "/restricted",
        },
        {
            directory: "unrestricted",
            rights: [ "rw*" ],
            path: "/unrestricted",
        },
    ],
    expose: [
        {
            protocol: "test.policy.Access",
            from: "self",
        },
    ],
}
