{
    program: {
        binary: "bin/gtest_adapter",
        args: [ "/pkg/bin/gtest_sample_tests" ],
    },
    use: [
        { runner: "elf" },
        { protocol: "/svc/fuchsia.process.Launcher" },
        { protocol: "/svc/fuchsia.logger.LogSink" },
        {
            directory: "/tmp",
            rights: [ "rw*" ],
        },
    ],
    expose: [
        {
            protocol: "/svc/fuchsia.test.Suite",
            from: "self",
        },
    ],
}
