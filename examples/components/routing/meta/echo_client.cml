// Component manifest for the `echo_client` example program, which sends a message to the Echo
// service.
{
    program: {
        binary: "bin/echo_client",
        args: [
            "Hippos",
            "rule!",
        ],
    },
    use: [
        { runner: "elf" },
        { protocol: "/svc/fidl.examples.routing.echo.Echo" },
    ],
}
