library fidl.test.during;

interface AddMethod {
    1: ExistingMethod();
    [Transitional]
    2: NewMethod();
};

interface RemoveMethod {
    1: ExistingMethod();
    [Transitional]
    2: OldMethod();
};

interface AddEvent {
    1: ExistingMethod();
    [Transitional]
    2: -> NewEvent();
};

interface RemoveEvent {
    1: ExistingMethod();
    [Transitional]
    2: -> OldEvent();
};
