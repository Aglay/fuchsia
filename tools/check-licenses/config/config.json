{
  "filesRegex": [],
  "skipFiles": [
    ".gitignore"
  ],
  "skipDirs": [
    ".git",
    "out",
    ".cipd"
  ],
  "textExtensionList": [
    "ac",
    "am",
    "asm",
    "bat",
    "c",
    "cc",
    "cpp",
    "dart",
    "gn",
    "gni",
    "go",
    "gyp",
    "gypi",
    "h",
    "hh",
    "in",
    "inc",
    "java",
    "js",
    "m",
    "md",
    "py",
    "rs",
    "sh",
    "txt"
  ],
  "maxReadSize": 6144,
  "separatorWidth": 80,
  "outputFilePrefix": "NOTICE",
  "outputFileExtension": "html.gz",
  "product": "astro",
  "singleLicenseFiles": [
    "license",
    "readme",
    "notice",
    "copying",
    "copyright"
  ],
  "customProjectLicenses" : [
    {
      "projectRoot": "prebuilt/third_party/rust/linux-x64",
      "licenseLocation": "share/doc/rust/COPYRIGHT"
    },
    {
      "projectRoot": "prebuilt/third_party/cmake/linux-x64",
      "licenseLocation": "doc/cmake-3.10/Copyright.txt"
    }
  ],
  "goldenLicenses": "golden",
  "licensePatternDir": "tools/check-licenses/golden/patterns",
  "baseDir": ".",
  "target": "all",
  "logLevel": "verbose"
}
