### `gn clean` the ZIRCON_BUILDROOT and FUCHSIA_BUILD_DIR

source "$(cd "$(dirname "${BASH_SOURCE[0]}")" >/dev/null 2>&1 && pwd)"/lib/vars.sh || exit $?
fx-config-read

set -ex

fx-gn clean "$ZIRCON_BUILDROOT"
fx-gn clean "$FUCHSIA_BUILD_DIR"
