library measuretape;

union TopLevelUnion {
    1: uint64 primitive;
    2: handle handle;
    3: StructWithString struct_with_string;
    4: StructWithOptString struct_with_opt_string;
    5: Table table;
};

struct StructWithString {
    string string;
};

struct StructWithOptString {
    string? opt_string;
};

table Table {
    5: uint32 primitive;
    2: reserved;
    1: reserved;
    3: string string;
    4: handle handle;
};
