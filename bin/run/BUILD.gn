import("//packages/package.gni")

executable("run_bin") {
  output_name = "run"
  sources = [
    "main.cc",
  ]
  deps = [
    "//application/lib/app",
    "//application/services:services_sync",
  ]
}

package("run") {
  deps = [
    ":run_bin",
  ]
  binaries = [ {
        name = "run"
      } ]
}
