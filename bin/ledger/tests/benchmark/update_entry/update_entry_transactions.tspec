{
  "test_suite_name": "fuchsia.ledger",
  "app": "ledger_benchmark_update_entry",
  "args": ["--entry-count=100", "--transaction-size=10", "--value-size=1000"],
  "categories": ["benchmark", "ledger"],
  "duration": 60,
  "measure": [
    {
      "type": "duration",
      "event_name": "transaction",
      "event_category": "benchmark",
      "split_samples_at": [1]
    },

    {
      "type": "duration",
      "event_name": "commit",
      "event_category": "benchmark",
      "split_samples_at": [1]
    },

    {
      "type": "duration",
      "event_name": "put",
      "event_category": "benchmark",
      "split_samples_at": [1]
    }
  ]
}
