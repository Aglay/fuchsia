library fuchsia.ui.input;

// Commands for conveying input events to a |Session|.
union Command {
  SendKeyboardInput send_keyboard_input;
  SendPointerInput send_pointer_input;
};

struct SendKeyboardInput {
  KeyboardEvent event;
};

struct SendPointerInput {
  PointerEvent event;
};
