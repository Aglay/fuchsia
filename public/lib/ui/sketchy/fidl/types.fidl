module sketchy;

import "lib/ui/gfx/fidl/types.fidl";

struct Touch {
  ui.gfx.vec2 position;
  uint32 nanoseconds;
  float32 pressure;
};

struct CubicBezier2 {
  ui.gfx.vec2 pt0;
  ui.gfx.vec2 pt1;
  ui.gfx.vec2 pt2;
  ui.gfx.vec2 pt3;
};

struct StrokePath {
  array<CubicBezier2> segments;
};
